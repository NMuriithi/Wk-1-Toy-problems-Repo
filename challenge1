function getGrade() {
    let mark = parseInt(prompt("Enter student mark (0-100): "));
    if (mark > 79) {
        return "A";
    } else if (mark > 59) {
        return "B";
    } else if (mark > 49) {
        return "C";
    } else if (mark > 39) {
        return "D";
    } else {
        return "E";
    }
}